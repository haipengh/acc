################################
# customer settings            #
################################
ORA_ROOT=/u002					# change the root of the oracle binary
export ORA_ROOT

DBF_ROOT=/u102					# change the root of the dbf
export DBF_ROOT

ARCH_ROOT=/u201					# change the root of the archivelog
export ARCH_ROOT

FRA_ROOT=/u201					# change the root of flashback recovery area
export FRA_ROOT

RMAN_ROOT=/u999					# change the root of flashback recovery area
export RMAN_ROOT

XFR_ROOT=/u999/data					# change the root of the data/transfer
export XFR_ROOT

ORACLE_BASE=${ORA_ROOT}/app/oracle
export ORACLE_BASE

ORACLE_HOME=${ORACLE_BASE}/product/11.2.0/dbhome_1	# change oracle home
export ORACLE_HOME

ORACLE_SID=DLEG1				# change ORACLE_SID
export ORACLE_SID

################################
# db generic settings          #
################################
PATH=${ORACLE_HOME}/bin:${PATH}
export PATH

ORACLE_TERM=vt100
export ORACLE_TERM

LD_LIBRARY_PATH=${ORACLE_HOME}/lib:${ORACLE_HOME}/ctx/lib:/usr/lib
export LD_LIBRARY_PATH

#LD_LIBRARY_PATH_64=${ORACLE_HOME}/lib
#export LD_LIBRARY_PATH_64

LIBPATH=${ORACLE_HOME}/lib
export LIBPATH

SHLIB_PATH=${ORACLE_HOME}/lib
export SHLIB_PATH

unset ORA_TZFILE
#ORA_TZFILE=${ORACLE_HOME}/oracore/zoneinfo/[little|big]/timezlrg_11.dat
#export ORA_TZFILE

ORA_NLS10=${ORACLE_HOME}/nls/data
export ORA_NLS10

#unset ORA_NLS33
#ORA_NLS33=${ORACLE_HOME}/ocommon/nls/admin/data
#export ORA_NLS33

NLS_LANG=AMERICAN_AMERICA.UTF8
export NLS_LANG

NLS_DATE_FORMAT="DD-MON-RRRR"
export NLS_DATE_FORMAT

NLS_NUMERIC_CHARACTERS=".,"
export NLS_NUMERIC_CHARACTERS

NLS_SORT="binary"
export NLS_SORT

TNS_ADMIN=${ORACLE_HOME}/network/admin		# change TNS_ADMIN
export TNS_ADMIN

################################
# file location settings       #
################################

DBF_BASE=${DBF_ROOT}/oradata/${ORACLE_SID}
export DBF_BASE

ARCH_BASE=${ARCH_ROOT}/oraarch/${ORACLE_SID}
export ARCH_BASE

FRA_BASE=${FRA_ROOT}/orafra/${ORACLE_SID}
export FRA_BASE

RMAN_DISK_DEST=${RMAN_ROOT}/backup/${ORACLE_SID}	# change RMAN_DISK_DEST
export RMAN_DISK_DEST

PATCH_TOP=/u999/patch					# change PATCH_TOP
export PATCH_TOP

XFR_BASE=${XFR_ROOT}/${ORACLE_SID}
export XFR_BASE

################################
# application generic settings #
################################
LEG_TOP=${MODULE_BASE}/${MODULE_TOP}
export LEG_TOP

DBA_TOP=${LEG_TOP}/dba
export DBA_TOP

PATH=${DBA_TOP}/bin:${PATH}
export PATH

SQLPATH=${DBA_TOP}/sql:${AM_LOCAL_TOP}/sql:.
export SQLPATH
