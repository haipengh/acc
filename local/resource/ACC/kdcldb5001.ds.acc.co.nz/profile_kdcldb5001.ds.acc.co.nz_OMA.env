export WHOIAM

ORA_ROOT=/u003					# change the root of the oracle binary
export ORA_ROOT

unset DBF_ROOT
#DBF_ROOT=/u101					# change the root of the dbf
#export DBF_ROOT

unset ARCH_ROOT
#ARCH_ROOT=/u201					# change the root of the archivelog
#export ARCH_ROOT

unset FRA_ROOT
