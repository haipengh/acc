HS_FDS_TRACE_LEVEL=OFF
HS_FDS_RECOVERY_ACCOUNT=RECOVER
HS_FDS_RECOVERY_PWD=Oracle_2_Sybase

#
# Environment variables required for Sybase
#
set SYBASE=/opt/sybase/OpenClient-15
